#!/usr/bin/env bash

function usage {
  echo "Usage: mongohqexport [mongohq_url] [-c collection_name ] [-h]"
  exit 1
}

# mongodb://user:password@stack.mongohq.com:port/database
function parse_url {
  user=
  password=
  host=
  port=
  database=
  mongoexport=
}

[[ $# -eq 0 ]] && usage
[[ $1 != "" ]] && parse_url $1

echo $mongoexport
